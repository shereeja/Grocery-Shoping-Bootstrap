            <script>

                document.addEventListener('DOMContentLoaded', (event) => {
                    $('#left-menu-btn').click(function () {
                        $('#left-menu').animate({ 'left': "0" }, 0);
                        $('#left-menu-overlay').fadeIn('slow');
                        $('body').addClass('toggle-scroll');
                    });

                    $('#close-left-menu, #left-menu-overlay').click(function () {
                        $('#left-menu').animate({ 'left': "-100%" }, 0);
                        $('#left-menu-overlay').fadeOut('slow');
                        $('body').removeClass('toggle-scroll');
                    });
                    $(window).on('resize', function () {
                        if ($(window).width() > 991) {
                            $('#left-menu').animate({ 'left': "-100%" }, 0);
                            $('#left-menu-overlay').fadeOut('slow');
                            $('body').removeClass('toggle-scroll');

                            $('#offcanvas-refine').animate({ 'bottom': "-100%" }, 0);
                            $('#left-menu-overlay').fadeOut('slow');
                        }
